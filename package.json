{
	"name": "svelte-permit-core",
	"version": "1.0.1",
	"description": "Core RBAC implementation for Permit with Svelte",
	"homepage": "https://github.com/ricky-fn/svelte-permit-core#readme",
	"bugs": {
		"url": "https://github.com/ricky-fn/svelte-permit-core/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ricky-fn/svelte-permit-core.git"
	},
	"license": "MIT",
	"author": {
		"name": "Ricky Jiang",
		"email": "ricky.jc2022@gmail.com"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"devDependencies": {
		"@commitlint/cli": "19.8.1",
		"@commitlint/config-conventional": "19.8.1",
		"@fortawesome/free-brands-svg-icons": "7.0.0",
		"@fortawesome/free-solid-svg-icons": "7.0.0",
		"@lucide/svelte": "0.525.0",
		"@rollup/plugin-dsv": "3.0.5",
		"@sveltejs/adapter-cloudflare": "^7.2.3",
		"@sveltejs/adapter-static": "3.0.8",
		"@sveltejs/kit": "2.22.2",
		"@sveltejs/vite-plugin-svelte": "5.1.0",
		"@tailwindcss/postcss": "4.1.13",
		"@tailwindcss/vite": "4.1.11",
		"archieml": "0.5.0",
		"autoprefixer": "10.4.21",
		"bits-ui": "2.8.10",
		"classnames": "2.5.1",
		"d3": "7.9.0",
		"husky": "9.1.7",
		"lodash.debounce": "4.0.8",
		"postcss": "8.5.6",
		"prettier": "3.6.2",
		"prettier-plugin-svelte": "3.4.0",
		"rollup": "4.44.2",
		"rollup-plugin-svg": "2.0.0",
		"runed": "0.29.2",
		"shiki": "3.9.2",
		"style-dictionary": "5.0.1",
		"svelte": "5.35.4",
		"svelte-fa": "4.0.4",
		"svelte-preprocess": "6.0.3",
		"tailwindcss": "4.1.11",
		"typescript": "5.8.3",
		"vite": "6.3.5",
		"vite-plugin-svgstring": "1.0.0",
		"publint": "0.3.2"
	},
	"engines": {
		"node": ">= 18.20.4"
	},
	"browserslist": "> 0.5%, last 4 versions, not ie <= 11, not ie_mob <= 11",
	"dependencies": {
		"@sveltejs/package": "^2.5.3",
		"permit-core": "1.2.1"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}